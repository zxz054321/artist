<!DOCTYPE html>
{% macro ng(contents) %}{{ '{{' ~ contents ~ '}}' }}{% endmacro %}
{% macro url(url) %}{{ app.request.baseUrl }}/{{ url }}{% endmacro %}
{% import _self as i %}
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>{% block title %}{% endblock %} - Artist</title>

    <link href="{{ app.request.baseUrl }}/bootstrap.min.css" rel="stylesheet">
    <link href="{{ app.request.baseUrl }}/admin.css" rel="stylesheet">

    {% block style %}{% endblock %}

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
{% block body %}{% endblock %}

<script src="http://apps.bdimg.com/libs/jquery/1.11.3/jquery.min.js"></script>
<script src="//cdn.bootcss.com/bootstrap/3.3.6/js/bootstrap.min.js"></script>

{% if app.debug %}
    <script src="//cdn.bootcss.com/angular.js/1.5.0/angular.js"></script>
{% else %}
    <script src="//cdn.bootcss.com/angular.js/1.5.0/angular.min.js"></script>
{% endif %}

<script src="{{ app.request.baseUrl }}/admin.js"></script>

{% block script %}{% endblock %}
</body>
</html>